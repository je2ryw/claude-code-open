{
  "name": "claude-code-open",
  "version": "2.1.32",
  "description": "Open source Claude Code CLI - Educational Purpose Only",
  "type": "module",
  "bin": {
    "claude": "./dist/cli.js",
    "claude-code-open": "./dist/cli.js",
    "claude-web": "./dist/web-cli.js",
    "claude-proxy": "./dist/proxy-cli.js"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "start": "node dist/cli.js",
    "dev": "tsx src/cli.ts",
    "web": "tsx src/web-cli.ts",
    "web:debug": "node --inspect --import tsx src/web-cli.ts",
    "web:start": "node dist/web-cli.js",
    "web:dev": "tsx watch src/web-cli.ts",
    "proxy": "tsx src/proxy-cli.ts",
    "proxy:start": "node dist/proxy-cli.js",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:integration": "vitest tests/integration",
    "test:unit": "vitest src",
    "test:coverage": "vitest --coverage",
    "test:watch": "vitest --watch",
    "test:e2e": "tsx tests/e2e/run-all.ts",
    "test:e2e:basic": "tsx tests/e2e/cli-basic.test.ts",
    "test:e2e:session": "tsx tests/e2e/cli-session.test.ts",
    "test:e2e:tools": "tsx tests/e2e/cli-tools.test.ts",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "claude",
    "cli",
    "ai",
    "anthropic",
    "code-assistant"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/kill136/claude-code-open"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.70.0",
    "@google/genai": "^1.38.0",
    "@ngrok/ngrok": "^1.7.0",
    "@resvg/resvg-js": "^2.6.2",
    "@resvg/resvg-wasm": "^2.6.2",
    "@trpc/client": "^11.0.0",
    "@trpc/server": "^11.0.0",
    "@vscode/tree-sitter-wasm": "^0.3.0",
    "ajv": "^8.17.1",
    "axios": "^1.6.0",
    "better-sqlite3": "^11.10.0",
    "chalk": "^5.3.0",
    "cheerio": "^1.0.0",
    "cli-highlight": "^2.1.11",
    "commander": "^12.0.0",
    "eventsource": "^4.1.0",
    "express": "^5.2.1",
    "express-validator": "^7.3.1",
    "file-type": "^18.0.0",
    "glob": "^10.3.0",
    "http-proxy-agent": "^7.0.2",
    "https-proxy-agent": "^7.0.6",
    "ink": "^5.0.0",
    "jszip": "^3.10.1",
    "lru-cache": "^11.2.4",
    "marked": "^12.0.0",
    "mime": "^4.1.0",
    "open": "^11.0.0",
    "proxy-from-env": "^1.1.0",
    "react": "^18.2.0",
    "sharp": "^0.33.0",
    "socks-proxy-agent": "^8.0.5",
    "tree-sitter": "^0.25.0",
    "tree-sitter-wasms": "^0.1.13",
    "turndown": "^7.2.2",
    "turndown-plugin-gfm": "^1.0.2",
    "uuid": "^9.0.1",
    "vectra": "^0.11.1",
    "web-tree-sitter": "0.22.6",
    "wink-bm25-text-search": "^3.1.2",
    "ws": "^8.18.3",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@anthropic-ai/claude-code": "^2.1.31",
    "@types/better-sqlite3": "^7.6.13",
    "@types/chokidar": "^1.7.5",
    "@types/eventsource": "^1.1.15",
    "@types/express": "^5.0.6",
    "@types/node": "^20.19.30",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.3.7",
    "@types/shell-quote": "^1.7.5",
    "@types/uuid": "^9.0.0",
    "@types/ws": "^8.18.1",
    "@vitejs/plugin-react": "^4.7.0",
    "@vitest/ui": "^2.1.9",
    "chokidar": "^5.0.0",
    "highlight.js": "^11.11.1",
    "ink-testing-library": "^4.0.0",
    "jsdom": "^27.4.0",
    "react-dom": "^18.3.1",
    "tsx": "^4.7.0",
    "typescript": "^5.3.0",
    "vite": "^6.4.1",
    "vitest": "^2.1.9"
  }
}
